{
  "name": "bhyve-cloudfunction",
  "version": "0.1.0",
  "description": "A Cloud Function gateway for the unofficial Orbit B-Hyve API indended for use with HomeAssistant",
  "main": "index.js",
  "keywords": [
    "orbit bhyve",
    "homeassistant",
    "cloud function"
  ],
  "scripts": {
    "start": "node -r dotenv/config index.js dotenv_config_path=./.env",
    "cleanmac": "find . -name '.DS_Store' -type f -delete",
    "standard": "standard --verbose --fix"
  },
  "author": "tj madsen",
  "license": "MIT",
  "dependencies": {
    "ajv": "^6.11.0",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "ws": "^7.2.1"
  },
  "devDependencies": {
    "standard": "^14.3.1"
  }
}
